{
    "presets": [
        [
            "@babel/preset-env",
            {
              "targets": {
                "esmodules": false,
                "node": "current"
              }
            }
          ]
    ],
    "plugins": [
        "@babel/transform-runtime",
        "@babel/plugin-proposal-class-properties",
        "babel-plugin-add-module-exports",
        "@babel/plugin-transform-classes"
    ]
}
